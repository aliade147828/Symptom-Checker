<body>
    
    <header>

        <nav class="navbar navbar-expand-lg navigation" id="navbar">
            <div class="container">

                <div class="nav-logo">
                    <a class="navbar-brand" href="index.html">
                        <img src="~/Images/Logo.png" alt="" class="img-fluid" width="75px">
                    </a>
                </div>
                <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarmain" aria-controls="navbarmain" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="icofont-navigation-menu"></span>
                </button>

                <div class="collapse navbar-collapse " id="navbarmain">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link " asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item"> <a class="nav-link " asp-area="" asp-controller="Home" asp-action="About">About</a></li>
                        @if (User.IsInRole("Admin"))
                        {
                            <li class="nav-item">
                                <a class="nav-link " asp-area="" asp-controller="role" asp-action="Index">manage roles</a>
                            </li>

                            <li class="nav-item">
                                <a class="nav-link " asp-area="" asp-controller="Admin" asp-action="Index">manage users</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link " asp-area="" asp-controller="Department" asp-action="Index">Department</a>
                            </li>
                            <li class="nav-item">
                                <a class="nav-link " asp-area="" asp-controller="Request" asp-action="Index">Requests</a>
                                <a class="nav-link " asp-area="" asp-controller="Appoinment" asp-action="Index">Requests</a>

                            </li>
                        }
                         @if (User.IsInRole("Admin") == false)
                        {
                            
                            <li class="nav-item"><a class="nav-link" href="service.html">Services</a></li>
                            <li class="nav-item"><a class="nav-link" href="contact.html">Search drug</a></li>
                            <li class="nav-item"><a class="nav-link" asp-area="" asp-controller="User" asp-action="FindADoctor">Find doctor</a></li>
                            <li class="nav-item">
                                <a class="nav-link " asp-area="" asp-controller="Request" asp-action="SendRequest">Send Request</a>
                            </li>
                        }
                        <li class="nav-item"><a class="nav-link " asp-area="" asp-controller="Home" asp-action="Contact">Contact</a></li>

                    </ul>
                    <partial name="_LoginPartial" />
                </div>
            </div>

        </nav>
        @if (User.IsInRole("Admin") == false)
        {


            <div class=" contact-icons">
                <a href="+201151988689"><img class="phone-icon" src="~/images/phone-call.png" width="50px" height="50px">      </a>

                <a href="http://wa.me/+2011511988689">  <img class="whatsapp-icon mt-2" src="~/images/whatsapp.png" width="50px" height="50px">   </a>

            </div>
        }

    </header>

    <div class="main">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>
