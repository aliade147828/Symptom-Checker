@model IEnumerable<AppoinmentViewModel>
@{
    ViewData["Title"] = "Index";
}

@section Styles{
    <link rel="stylesheet" href="//cdn.datatables.net/1.13.4/css/jquery.dataTables.min.css" />

}

<div class="container my-3">
    <div class="row mb-3">
        <div class="col-6">
            Appoinments List
        </div>
       
    </div>

    <table id="myTable" class="table table-striped table-hover">
        <thead>
            <tr>
                <th> Name</th>
                <th> Email</th>
                <th> Phone</th>
                <th>Doctor Name</th>
                <th>Date</th>
                <th>Status</th>
                <th></th>

            </tr>
        </thead>
    </table>


</div>
@section Scripts {
    <script src="~/js/AppoinmentDataTable.js"> </script>
    <script src="//cdn.datatables.net/1.13.4/js/jquery.dataTables.min.js"> </script>

}
